#include <stdio.h>
#include <stdlib.h>

/**
 * main - Entry point
 *
 * Return: Always 0. (Success)
 */

int main(void)
{
char *str = "John";
int i, j;
char *str1 = "My School";

_printf("Hello Mr. %c\n", 'A');
_printf("%c initializes my name\n", 'J');
_printf("Just a test\n");
for (i = 0; i < 5; i++)
{
_putchar(str[i]);
}

_putchar('\n');

for (j = 0; j < 10 ; j++)
{
_putchar(str1[j]);
}

_putchar('\n');

return (0);
}
